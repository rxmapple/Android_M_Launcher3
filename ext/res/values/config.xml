    <bool name="config_show_dynamic_calendar">true</bool>

    <string name="default_dynamic_clock" translatable="false">com.google.android.deskclock/com.android.deskclock.DeskClock</string>
    <bool name="config_show_dynamic_clock">true</bool>

    <integer-array translatable="false" name="config_defaultClockTime">
        <item>10</item> <!-- Default Hour -->
        <item>10</item> <!-- Default Minute -->
